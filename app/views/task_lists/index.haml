-content_for :banner do
  .gantt
    = @chart.to_html(1, 32, 30, 0)

-content_for :column do
  = task_list_column(@current_project,@task_lists,@sub_action,@task_list)

- if @task_lists.any?
  = main_task_list(@current_project,@task_lists,@sub_action)
- elsif @sub_action == 'all'
  = task_list_primer(@current_project)
- else
  %p= t('.empty')