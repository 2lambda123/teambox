- form_for [@current_project,@page,@divider], :html => { :id => "new_divider_form" } do |f|
  = page_slot_fields("new_divider_form")
  .area
    = divider_fields(f)
    .submit{ :id => "divider_submit" }
      = f.submit t('.submit')
      = t('common.or')
      = link_to t('common.cancel'), '#', :class => 'cancelPageWidget'
    .clear  
  .clear