- if @pending_projects
  .pending_projects
    = list_pending_projects(@pending_projects)

%h2
  News feed for all your projects
    
#activities
  = list_activities(@activities)

- content_for :column do
  = new_project_link
  .project_list
    %h2= t '.projects'
    = list_projects(@projects)
  .subscribe
    = link_to t('.subscribe'), projects_path(:format => :rss)